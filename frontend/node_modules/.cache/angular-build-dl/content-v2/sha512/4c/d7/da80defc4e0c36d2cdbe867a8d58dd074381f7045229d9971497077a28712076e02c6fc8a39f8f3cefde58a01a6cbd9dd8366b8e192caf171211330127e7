\n      }\r\n    });\r\n    this.loading = true;\r\n    this.updateMenu(menu);\r\n  }\r\n}\r\n","<!-- Container-fluid starts-->\r\n<div class=\"container-fluid\">\r\n    <div class=\"row\">\r\n        <div class=\"col-sm-12\">\r\n            <div class=\"card\">\r\n                <div class=\"card-header\">\r\n                    <h5>Menyu qo‘shish</h5>\r\n                </div>\r\n                <div class=\"card-body\">\r\n                    <div class=\"needs-validation\">\r\n                        <div class=\"form-group row\">\r\n                            <label for=\"validationCustom0\" class=\"col-xl-3 col-md-4\"><span>*</span> Ramzi uz</label>\r\n                            <input class=\"form-control col-md-8\" [(ngModel)]=\"dataService.menuNew.symbol_lang.uz\" id=\"validationCustom0\" type=\"text\" required=\"\">\r\n                        </div>\r\n                        <div class=\"form-group row\">\r\n                            <label for=\"validationCustom01\" class=\"col-xl-3 col-md-4\"><span>*</span> Ramzi ru</label>\r\n                            <input class=\"form-control col-md-8\" [(ngModel)]=\"dataService.menuNew.symbol_lang.ru\" id=\"validationCustom01\" type=\"text\" required=\"\">\r\n                        </div>\r\n                        <div class=\"form-group row\">\r\n                            <label for=\"validationCustom02\" class=\"col-xl-3 col-md-4\"><span>*</span> Ramzi en</label>\r\n                            <input class=\"form-control col-md-8\" [(ngModel)]=\"dataService.menuNew.symbol_lang.en\" id=\"validationCustom02\" type=\"text\" required=\"\">\r\n                        </div>\r\n                        <div class=\"form-group row\">\r\n                            <label for=\"validationCustom1\" class=\"col-xl-3 col-md-4\"><span>*</span> Sarlavha uz</label>\r\n                            <textarea class=\"form-control col-md-8\" [(ngModel)]=\"dataService.menuNew.title.uz\" id=\"validationCustom1\" required=\"