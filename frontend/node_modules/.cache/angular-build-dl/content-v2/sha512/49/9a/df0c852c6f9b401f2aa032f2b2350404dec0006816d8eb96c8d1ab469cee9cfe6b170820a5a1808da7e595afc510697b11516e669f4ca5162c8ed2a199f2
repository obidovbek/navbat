imeout(()=>{this.getMenu(i+1)},1000);\r\n      }else{\r\n        // alert(\"Bazaka ulana olmadim, oflayn rejimda ishlayapti!\");\r\n        this.dataService.menu = await localStorage.getItem(\"menu\");\r\n        this.dataService.menu = JSON.parse(this.dataService.menu);\r\n      }\r\n    });\r\n  }\r\n\r\n}","<!-- <div (click)=\"selectLanguage('uz')\">O‘zbekcha</div>\r\n<div (click)=\"selectLanguage('ru')\">Русский</div>\r\n<div (click)=\"selectLanguage('en')\">English</div> -->\r\n\r\n<div class=\"lang\">\r\n    <!-- <img class=\"img\" alt=\"\"> -->\r\n    <div class=\"img\"></div>\r\n    <p class=\"lang-text\">Kelajakni birgalikda quramiz!</p>\r\n    <div (click)=\"selectLanguage('uz')\" class=\"lang-inner\">O'zbekcha</div>\r\n    <div (click)=\"selectLanguage('ru')\" class=\"lang-inner\">Русский</div>\r\n    <div (click)=\"selectLanguage('en')\" class=\"lang-inner\">English</div>\r\n</div>  \r\n"]}